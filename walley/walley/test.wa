;;(car (car (quote ((x)) )  ) )
(def append 
     (lambda (a b)
       (if (null? a)
	   b
	 (cons (car a)
	       (append (cdr a)
		       b)))))
(defmacro defn
  [(var_name args . body)
  `(def ~var_name (lambda ~args ~@body))
  ]
  )

(defn add [a b] (+ a b))
(add 3 4)
(defn sub [a b] (- a b))
(sub (/ 3 6) (+ 3 4))
