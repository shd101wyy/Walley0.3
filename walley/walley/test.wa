(def append 
     (lambda (a b)
       (if (null? a)
	   b
	 (cons (car a)
	       (append (cdr a)
		       b)))))
(defmacro defn
  [(var_name args . body)
  ;`(def ~var_name (lambda ~args ~@body))
  `(~@body)
  ]
  )
