(set! >= 
	(let [old->= >=
		  >=-iter (lambda [args cur]
		  			(if (null? args) true
		  				(if (old->= cur (car args))
		  					(>=-iter (cdr args) (car args))
		  					false)
		  				))]
		(lambda [. args]
			(if (null? args)
				(display "Please provide arguments for >=")
				(>=-iter (cdr args) (car args))))))

(display (>=  5 4 4))